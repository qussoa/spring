$(function() {
	
	/* 입력박스에 키보드로 뭔가를 입력할 때마다 발생하는 이벤트
	 *	keydown 
	 *		키보드를 누르는 순간 발생
	 *	keypress
	 *		글자가 입력이 되어서 컴퓨터로 입력되는 순간 발생
	 *		어떤 글자가 입력되었는가를 캐치하기 위한 이벤트
	 *	keyup
	 * 		키보드에서 손을 떼는 순간
	 */
	$("#io_dcode").keypress(function() {
		// 키보드에서 어떤 문자를 입력하면 event의 keyCode라는 속성에
		// 문자의 ASCII 코드 값이 저장되어 전달된다
		if(event.keyCode == 13){
			let str = $(this).text()
			alert(str)
		}
		
	})
	
	
	
	
	
	
	
})







